{
  "src_folders": [
    "src"
  ],
  "webdriver": {
    "start_process": true,
    "server_path": "node_modules/.bin/chromedriver",
    "port": 9515,
    "timeout_options": {
      "timeout": 15000,
      "retry_attempts": 3
    }
  },
  "custom_commands_path": "commands",
  "globals_path": "./src/globals.js",
  "page_objects_path": "pages",
  "globals": {
    "abortOnAssertionFailure": false,
    "abortOnElementLocateError": false,
    "waitForConditionPollInterval": 500,
    "waitForConditionTimeout": 5000
  },
  "test_settings": {
    "default": {
      "desiredCapabilities": {
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "chromeOptions": {
          "args": [
            "--load-extension=C:/Users/wnijmeijer/Google Drive/Documents/GitHub/coveo-response-checker",
            "safebrowsing-disable-download-protection"
          ],
          "prefs": {
            "download": {
              "prompt_for_download": false,
              "default_directory": "D:/export/chrome/"
            },
            "browser": {
              "set_download_behavior": {
                "behavior": "allow"
              }
            }
          }
        }
      }
    }
  },
  "test_workers": true
}